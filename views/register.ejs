<h1>Register</h1>
<form action="/register" method="POST">
  <div>
    <label for="fname">First Name</label>
    <input type="text" id="fname" name="fname" required>
  </div>
  <div>
    <label for="lname">Last Name</label>
    <input type="text" id="lname" name="lname" required>
  </div>
  <div>
    <label for="university">University</label>
    <input type="text" id="university" name="university" required>
  </div>
  <div>
    <label for="email">Email</label>
    <input type="email" id="email" name="email" required>
  </div>
  <div>
    <label for="userID">User ID</label>
    <input type="text" id="userID" name="userID" required>
  </div>
  <div>
    <label for="password">Password</label>
    <input type="password" id="password" name="password" required>
  </div>
  <div>
    <label for="fallMealName">Fall Meal Plan</label>
    <select id="fallMealName" name="fallMealName">
      <option value="300-flex-plus">300 Flex PLUS</option>
      <option value="300-flex">300 Flex</option>
      <option value="225-flex-plus">225 Flex PLUS</option>
      <option value="225-flex">225 Flex</option>
    </select>
    <!-- <input type="text" id="fallMealName" name="fallMealName" required> -->
  </div>
  <div>
    <label for="springMealName">Spring Meal Plan</label>
    <select id="springMealName" name="springMealName">
      <option value="300-flex-plus">300 Flex PLUS</option>
      <option value="300-flex">300 Flex</option>
      <option value="225-flex-plus">225 Flex PLUS</option>
      <option value="225-flex">225 Flex</option>
    </select>
    <!-- <input type="text" id="springMealName" name="springMealName" required> -->
  </div>
  <div>
    <label for="campusCash">Campus Cash Amount: </label>
    <input type="text" id="campusCash" name="campusCash" required>
  </div>
  <button type="submit">Register</button>
</form>
<a href="/login">Login</a>